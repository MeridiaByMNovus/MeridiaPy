{
  "name": "meridia-py",
  "version": "1.1.5",
  "type": "module",
  "main": "./client.js",
  "module": "./client.js",
  "exports": {
    ".": {
      "types": "./client.d.ts",
      "default": "./client.js"
    },
    "./server": {
      "types": "./server.d.ts",
      "default": "./server.js"
    }
  },
  "readme": "All-in-one Python Language Server + Monaco Editor bundle",
  "repository": {
    "type": "git",
    "url": "https://github.com/MeridiaByMNovus/MeridiaPy.git"
  },
  "author": {
    "name": "MNovus",
    "email": "off.manojkhandelwal@gmail.com",
    "url": "https://github.com/MeridiaByNovus"
  },
  "homepage": "https://github.com/MeridiaByMNovus/MeridiaPy#readme",
  "dependencies": {
    "express": "~4.18.2",
    "monaco-editor-workers": "~0.41.0",
    "pyright": "~1.1.322",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "request-light": "~0.7.0",
    "vscode-json-languageservice": "~5.3.5",
    "vscode-languageserver": "~8.1.0",
    "ws": "~8.13.0"
  },
  "devDependencies": {
    "@types/ws": "~8.5.5",
    "@types/express": "~4.17.17",
    "@types/react": "~18.2.20",
    "@types/react-dom": "~18.2.7",
    "ts-node": "~10.9.1",
    "vscode-languageserver-types": "~3.17.3"
  },
  "typesVersions": {
    "*": {
      ".": [
        "./client.d.ts"
      ],
      "./server": [
        "./server.d.ts"
      ]
    }
  },
  "keywords": [
    "python",
    "client",
    "python-language-server",
    "monaco-python",
    "monaco"
  ],
  "license": "MIT",
  "volta": {
    "node": "18.17.1",
    "npm": "9.6.7"
  },
  "scripts": {
    "clean": "shx rm -fr dist *.tsbuildinfo",
    "compile": "tsc --build tsconfig.src.json",
    "build:msg": "echo Building main examples:",
    "build": "npm run build:msg && npm run clean && npm run compile",
    "start:server:python": "node --loader ts-node/esm src/python/server.ts"
  }
}
